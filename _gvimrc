set go = 

" Default size when opening
set lines=66
set columns=137

" Base colors
" hi NonText ctermbg=NONE

" Vertsplit Theme
hi LineNr guibg=bg
set foldcolumn=0
hi foldcolumn guibg=bg
hi VertSplit guibg=bg guifg=bg

" Font
set guifont=Cascadia_Code:h11
    
" Disable edge of screen bell
set belloff=all
